{
  "AppName": "SampleApp",
  "Version": "1.0.0",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Error"
    },
    "File": {
      "Path": "/logs/app.log",
      "MaxSizeMB": 10
    }
  },
  "Database": {
    "ConnectionString": "Server=localhost;Database=SampleDB;User Id=admin;Password=pass123;",
    "Provider": "SQLServer",
    "Settings": {
      "RetryCount": 3,
      "TimeoutSeconds": 30
    }
  },
  "Features": {
    "EnableFeatureX": true,
    "EnableFeatureY": false
  },
  "Users": [
    {
      "Id": 1,
      "Name": "Alice",
      "Roles": [ "Admin", "Editor" ]
    },
    {
      "Id": 2,
      "Name": "Bob",
      "Roles": [ "Viewer" ]
    }
  ],
  "Modules": [
    {
      "ModuleName": "Reporting",
      "Enabled": true,
      "Settings": {
        "RefreshInterval": 15,
        "ExportFormats": [ "PDF", "Excel" ]
      }
    },
    {
      "ModuleName": "Analytics",
      "Enabled": false,
      "Settings": {
        "RefreshInterval": 30,
        "ExportFormats": [ "CSV" ]
      }
    }
  ]
}
